{
	"name": "tabella-nextjs",
	"description": "To create and view schemas in SQL and more",
	"version": "0.4.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 5757 --turbopack",
		"build": "next build --turbopack",
		"start": "next start",
		"postinstall": "next telemetry disable",
		"secret": "npx @better-auth/cli@latest secret",
		"db:schema": "npx @better-auth/cli@latest generate --config src/libs/better-auth/auth.ts",
		"db:push": "npx drizzle-kit push && npx drizzle-kit generate",
		"db:studio": "npx drizzle-kit studio"
	},
	"dependencies": {
		"better-auth": "1.4.7",
		"dotenv": "17.2.3",
		"drizzle-orm": "0.45.1",
		"next": "15.5.8",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"tailwind-merge": "3.4.0",
		"tailwind-variants": "3.2.2"
	},
	"devDependencies": {
		"@tailwindcss/postcss": "4.1.18",
		"@types/node": "25.0.2",
		"@types/pg": "8.16.0",
		"@types/react": "19.2.7",
		"@types/react-dom": "19.2.3",
		"drizzle-kit": "0.31.8",
		"pg": "8.16.3",
		"tailwindcss": "4.1.18",
		"tsk": "0.3.2",
		"typescript": "5.9.3"
	},
